{
  "name": "gost",
  "version": "0.0.1",
  "description": "A minimalistic Ghost blog theme",
  "main": "index.js",
  "engines": {
    "ghost": ">=1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/goLance-LLC/gost.git"
  },
  "keywords": [
    "ghost",
    "blog",
    "theme"
  ],
  "author": {
    "name": "goLance",
    "email": "admin@golance.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/goLance-LLC/gost/issues"
  },
  "homepage": "https://github.com/goLance-LLC/gost#readme",
  "config": {
    "posts_per_page": 10
  },
  "devDependencies": {
    "autoprefixer-stylus": "^0.14.0",
    "browser-sync-brunch": "0.0.9",
    "brunch": "^2.10.10",
    "javascript-brunch": "^2.10.0",
    "npm-run-all": "^4.0.2",
    "purify-css": "^1.2.5",
    "rimraf": "^2.6.1",
    "stylus-brunch": "^2.10.0"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "brunch watch",
    "build": "run-s build:clean build:run build:purify-css build:uglify-es",
    "build:clean": "rimraf ./assets/build",
    "build:run": "brunch build --production",
    "build:uglify-es": "uglifyjs --compress --mangle --output assets/build/scripts.js -- assets/build/scripts.js",
    "build:purify-css": "purifycss assets/build/styles.css \"**/*.hbs\" --out assets/build/styles.css --min"
  },
  "dependencies": {
    "goui": "git://github.com/goLance-LLC/goui.git#master",
    "infinite-scroll": "^3.0.1",
    "medium-zoom": "^0.1.7",
    "normalize.css": "^7.0.0",
    "uglify-es": "^3.1.0"
  }
}
